using System;

namespace project
{
    class test
    {
        public static void Main()
        {
            int X= Convert.ToInt32(Console.ReadLine());
            Calc (X);
        }
        private static void Calc (int x)
        {
            double result = Math.Pow(x, 2)+(2*x)-4;
            Console.WriteLine(result);
        }
    }
}
